<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    
    <% if flash.any? %>
      <% flash.each do |msgtype, msg| %>
        <p><%= msg %></p>
      <% end %>
    <% end %>
    
    <%= yield %>
  
    <%= debug(params) %>
      
  </body>
</html>
